PKG_NAME=xterm
PKG_VERSION=308
PKG_URL="ftp://invisible-island.net/xterm/$PKG_NAME-$PKG_VERSION.tgz"
PKG_REV=1
PKG_BUILD_DEPENDS="toolchain libX11 libXaw"
PKG_RUN_DEPENDS="$TARGET_LIBC libX11 libXaw libXpm"
PKG_PRIORITY=optional
PKG_SECTION=x11
PKG_SHORTDESC="A terminal emulator for the X Window System"
PKG_LONGDESC="It provides DEC VT102 and Tektronix 4014 compatible terminals for programs that can't use the window system directly."
