PKG_PARENT=eglibc
PKG_NAME=${PKG_PARENT}-gconv-minimal
PKG_PRIORITY=optional
PKG_SECTION=system
PKG_SHORTDESC="GNU C Library: selected gconv libraries for i18n"
PKG_LONGDESC="This package contains the minimal gconv libraries, which are necessary to make i18n work. These include ISO8859-1 ISO8859-15 and UNICODE."
