PKG_PARENT=glibc
PKG_NAME=${PKG_PARENT}-gconv-extra
PKG_PRIORITY=optional
PKG_SECTION=system
PKG_REPLACES="eglibc-gconv-extra"
PKG_CONFLICTS="eglibc-gconv-extra"
PKG_SHORTDESC="GNU C Library: gconv libraries for i18n"
PKG_LONGDESC="This package contains all gconv libraries as extension to gconv-minimal, necessary for subtitles in xbmc as well as epg vom dvbt."
