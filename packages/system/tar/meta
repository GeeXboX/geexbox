PKG_NAME=tar
PKG_VERSION=1.27.1
PKG_URL="http://ftp.gnu.org/gnu/$PKG_NAME/$PKG_NAME-$PKG_VERSION.tar.xz"
PKG_REV=1
PKG_RUN_DEPENDS="$TARGET_LIBC"
PKG_BUILD_DEPENDS="toolchain"
PKG_DEPENDS="zlib"
PKG_PRIORITY=optional
PKG_SECTION=system
PKG_SHORTDESC="tar"
PKG_LONGDESC="GNU Tar provides the ability to create tar archives, as well as various other kinds of manipulation. For example, you can use Tar on previously created archives to extract files, to store additional files, or to update or list files which were already stored. "
