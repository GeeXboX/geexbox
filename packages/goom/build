#!/bin/sh

. config/options

$SCRIPTS/build toolchain

export ac_cv_path_SDL_CONFIG=no
export ac_cv_path_XMMS_CONFIG=no

cd $BUILD/$1*
./configure --host=$TARGET_NAME \
            --build=$HOST_NAME \
            --prefix=$LIB_PREFIX \
            --enable-static \
            --disable-shared
make
make install
