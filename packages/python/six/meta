PKG_NAME=six
PKG_VERSION=1.9.0
PKG_URL="https://pypi.python.org/packages/source/s/$PKG_NAME/$PKG_NAME-$PKG_VERSION.tar.gz"
PKG_REV=1
PKG_DEPENDS="$TARGET_LIBC Python"
PKG_BUILD_DEPENDS="toolchain"
PKG_PRIORITY=optional
PKG_SECTION=python
PKG_SHORTDESC="a Python 2 and 3 compatibility library"
PKG_LONGDESC="It provides utility functions for smoothing over the differences between the Python versions with the goal of writing Python code that is compatible on both Python versions."
