PKG_PARENT=libusb
PKG_NAME=${PKG_PARENT}-host
PKG_NO_PARENT_DEPENDS=yes
PKG_BUILD_DEPENDS="toolchain"
PKG_PRIORITY=optional
PKG_SECTION=libs
PKG_SHORTDESC="userspace USB programming library - host version"
PKG_LONGDESC="Library for programming USB applications without the knowledge of Linux kernel internals."
