#!/bin/sh

. config/options

cd $BUILD/$1*

if pkg_uses xine-lib vdpau; then
  do_install usr/lib/xine/plugins/2.0/xineplug_decode_vdpau*.so
  do_install usr/lib/xine/plugins/2.0/xineplug_vo_out_vdpau.so
else
  echo "Please rebuild xine-lib with VDPAU support to build this package."
  exit 1
fi

exit 0
