PKG_NAME=host-nss
PKG_VERSION=`( get_meta nss; echo $PKG_VERSION )`
PKG_URL="http://ftp.mozilla.org/pub/mozilla.org/security/nss/releases/NSS_3_17_RTM/src/nss-${PKG_VERSION}.tar.gz"
PKG_REV=1
PKG_SECTION="host-packages"
PKG_RUN_DEPENDS="$TARGET_LIBC"
PKG_BUILD_DEPENDS="toolchain host-sqlite host-nspr"
