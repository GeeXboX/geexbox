#!/bin/sh

. config/options

cd $BUILD/$1*

export PYTHON="$ROOT/$TOOLCHAIN/bin/python"
export am_cv_python_pythondir="/usr/lib/python2.6/site-packages"
export am_cv_python_pyexecdir="/usr/lib/python2.6/site-packages"

do_configure \
            --disable-Werror \
            --disable-api-docs \
            --disable-html-docs \

make
make_install
