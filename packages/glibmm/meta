PKG_NAME=glibmm
PKG_VERSION=2.18.1
PKG_REV=1
PKG_DEPENDS="$TARGET_LIBC GLib libsigc++"
PKG_BUILD_DEPENDS="toolchain GLib libsigc++"
PKG_PRIORITY=optional
PKG_SECTION=libs
PKG_SHORTDESC="C++ wrapper for the GLib toolkit"
PKG_LONGDESC="GLib is a low-level general-purpose library used mainly by GTK+/GNOME applications, but is useful for other programs as well. glibmm is the C++ wrapper for GLib"
