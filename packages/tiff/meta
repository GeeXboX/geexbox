PKG_NAME=tiff
PKG_VERSION=4.0.0alpha
PKG_REV=1
PKG_BUILD_DEPENDS="toolchain jpeg zlib"
PKG_DEPENDS="$TARGET_LIBC jpeg zlib"
PKG_PRIORITY=optional
PKG_SECTION=libs
PKG_SHORT_DESC="Tag Image File Format (TIFF) library"
PKG_LONG_DESC="libtiff is a library providing support for the Tag Image File Format (TIFF), a widely used format for storing image data.  This package includes the shared library."
