#!/bin/sh

. config/options
get_meta $1
export UNINSTALLED_GLIB_SRCDIR=$ROOT/$BUILD/GLib-2.37.7

cd $PKG_BUILD_DIR
autoreconf --install --force
intltoolize --force
autoreconf
do_configure \
	     --without-mbm \
             --without-qmi \
	     --without-mbim \
             
             
make
make_install
