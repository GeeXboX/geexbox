PKG_URL="$SFNET_SRCS/gimp-print/${PKG_NAME}-${PKG_VERSION}.tar.bz2"
PKG_REV=1
PKG_BUILD_DEPENDS="toolchain"
PKG_DEPENDS="ijs cups"
PKG_PRIORITY=standard
PKG_SECTION=drivers
PKG_SHORTDESC="high quality printer drivers for Linux"
PKG_LONGDESC="Gutenprint is a package of high quality printer drivers for Linux, BSD, Solaris, Macintosh OS X, IRIX, and other UNIX-alike operating systems. In many cases, these drivers rival or exceed the OEM drivers in quality and functionality."
