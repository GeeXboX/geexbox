#!/bin/sh

. config/options

mkdir -p $INSTALL/usr/lib
cp -P $BUILD/$1*/libcrypto.so* $INSTALL/usr/lib
cp -P $BUILD/$1*/libssl.so* $INSTALL/usr/lib

mkdir -p $INSTALL/usr/lib/ssl/engines
for engine in 4758cca aep atalla cswift gmp chil nuron sureware ubsec; do
  cp -P $BUILD/$1*/engines/lib${engine}.so $INSTALL/usr/lib/ssl/engines
done
