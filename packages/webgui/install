#!/bin/sh

. config/options

mkdir -p $INSTALL/var/www/img/big
mkdir -p $INSTALL/var/www/cgi-bin
cp -f $PACKAGES/$1/config/*.html $INSTALL/var/www
cp -f $PACKAGES/$1/config/*.css $INSTALL/var/www
cp -rf $PACKAGES/$1/config/img/* $INSTALL/var/www/img
cp -f $PACKAGES/$1/config/cgi-bin/*.cgi $INSTALL/var/www/cgi-bin
cp -f $BUILD/$1/*.cgi $INSTALL/var/www/cgi-bin
