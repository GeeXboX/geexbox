#!/bin/sh

. config/options

mkdir -p $INSTALL/var/www/cgi-bin
mkdir -p $INSTALL/var/www/js
mkdir -p $INSTALL/var/www/style/geexbox
cp -f $PACKAGES/$1/config/*.html $INSTALL/var/www
cp -f $PACKAGES/$1/config/js/*.js $INSTALL/var/www/js
cp -fR $PACKAGES/$1/config/style/geexbox/* $INSTALL/var/www/style/geexbox/
cp -f $BUILD/$1/*.cgi $INSTALL/var/www/cgi-bin

exit 0
