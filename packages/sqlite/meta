PKG_NAME=sqlite
PKG_VERSION=3.6.23.1
PKG_REV=1
PKG_BUILD_DEPENDS="toolchain"
PKG_DEPENDS="$TARGET_LIBC"
PKG_PRIORITY=optional
PKG_SECTION=libs
PKG_SHORT_DESC="SQLite shared library"
PKG_LONG_DESC="SQLite is a C library that implements an SQL database engine. Programs that link with the SQLite library can have SQL database access without running a separate RDBMS process."
