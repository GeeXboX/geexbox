#!/bin/sh

. config/options

$SCRIPTS/build toolchain

export MAKEFLAGS=-j1

cd $BUILD/$1*
./configure2 --host=$TARGET_NAME \
             --build=$HOST_NAME \
             --prefix=$LIB_PREFIX \
             --enable-static \
             --disable-shared \
             --disable-debug \
             --cc=$CC \

make
make install
