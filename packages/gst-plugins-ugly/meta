PKG_NAME=gst-plugins-ugly
PKG_VERSION=1.0.1
PKG_URL="http://gstreamer.freedesktop.org/src/gst-plugins-ugly/gst-plugins-ugly-${PKG_VERSION}.tar.xz"
PKG_REV=1
PKG_DEPENDS="gstreamer gst-plugins-base GLib libmad libdvdread a52dec libcdio libmpeg2"
PKG_RUN_DEPENDS="$TARGET_LIBC"
PKG_BUILD_DEPENDS="toolchain"
PKG_PRIORITY=optional
PKG_SECTION=multimedia
PKG_SHORTDESC="GStreamer libraries from the ugly set"
PKG_LONGDESC="Set of plug-ins that have good quality and correct functionality, but distributing them might pose problems"
