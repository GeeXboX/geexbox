#!/bin/sh

. config/options

$SCRIPTS/build toolchain

cd $BUILD/$1*
./configure --host=$TARGET_NAME \
            --build=$HOST_NAME \
            --prefix=$LIB_PREFIX \
            --enable-shared \
            --disable-static \
            --disable-log \
            --disable-debug-log \
            --disable-examples-build \

make
strip_libs .
make install
