PKG_NAME=edbus
PKG_URL_REV=`( get_meta EFL; echo $PKG_URL_REV )`
PKG_VERSION=1.2+${PKG_URL_REV}
PKG_URL_PROTO=svn
PKG_URL="http://svn.enlightenment.org/svn/e/trunk/e_dbus"
PKG_REV=1
PKG_DEPENDS="$TARGET_LIBC eina ecore dbus"
PKG_BUILD_DEPENDS="toolchain"
PKG_PRIORITY=optional
PKG_SECTION=libs
PKG_SHORTDESC="EFL D-Bus wrapper"
PKG_LONGDESC="This is the start of some basic convenience wrappers around dbus to ease integrating dbus with EFL based applications."
