#!/bin/sh

. config/options

$SCRIPTS/build toolchain
$SCRIPTS/build libpthreads-stubs
$SCRIPTS/build xcb-proto

cd $BUILD/$1*
./configure --host=$TARGET_NAME \
            --build=$HOST_NAME \
            --prefix=$LIB_PREFIX \
            --enable-shared \
            --disable-static \

make
$STRIP src/.libs/*.so*
make install
