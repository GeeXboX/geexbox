#!/bin/sh

. config/options

$SCRIPTS/install freetype
$SCRIPTS/install tiff
$SCRIPTS/install zlib
$SCRIPTS/install dbus
$SCRIPTS/install GLib
$SCRIPTS/install sqlite
$SCRIPTS/install freetype
$SCRIPTS/install fontconfig

cd $BUILD/$1*

do_install usr/bin/qml
do_install usr/imports/*

do_install usr/lib/libphonon.so*
do_install usr/lib/libQt*.so*

do_install usr/share/qt4/translations/*
