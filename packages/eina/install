#!/bin/sh

. config/options

SVN_RELEASE="-ver-pre-svn-05"
[ $TARGET_LIBC = uClibc ] && ELIBC=uclibc || ELIBC=gnueabi

mkdir -p $INSTALL/usr/lib
cp -PR $BUILD/$1*/objdir-target/src/lib/.libs/libeina*.so* $INSTALL/usr/lib
for i in chained_pool pass_through fixed_bitmap; do
  mkdir -p $INSTALL/usr/lib/eina/modules/mp/$i/linux-${ELIBC}-${TARGET_FAMILY}${SVN_RELEASE}
  cp -PR $BUILD/$1*/objdir-target/src/modules/mp/$i/.libs/module.so $INSTALL/usr/lib/eina/modules/mp/$i/linux-${ELIBC}-${TARGET_FAMILY}${SVN_RELEASE}
done
