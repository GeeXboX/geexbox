#!/bin/sh

. config/options

$SCRIPTS/install dbus
$SCRIPTS/install hal

mkdir -p $INSTALL/usr/bin
cp -f $BUILD/$1/$1 $INSTALL/usr/bin/
cp $PACKAGES/$1/scripts/hmount $INSTALL/usr/bin/
cp $PACKAGES/$1/scripts/humount $INSTALL/usr/bin/

mkdir -p $INSTALL/etc
cp $PACKAGES/$1/config/autoplay $INSTALL/etc
