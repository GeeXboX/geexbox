#!/bin/sh

. config/options

$SCRIPTS/build toolchain

export as_ac_File=yes

cd $BUILD/$1*
./configure --host=$TARGET_NAME \
            --build=$HOST_NAME \
            --prefix=$LIB_PREFIX \
            --enable-shared \
            --enable-static \
            --with-xorg-module-dir=$XORG_PATH_MODULES
make
