#!/bin/sh

. config/options

$SCRIPTS/build toolchain
$SCRIPTS/build libusb

cd $BUILD/$1*
./configure --host=$TARGET_NAME \
            --build=$HOST_NAME \
            --prefix=$LIB_PREFIX \
            --without-x \
            --without-ncurses \
            --with-drivers=all,!picoLCD,!Trefon,!USBLCD,!LCD2USB,!BWCT
make
