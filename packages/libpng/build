#!/bin/sh

. config/options

$SCRIPTS/build toolchain
$SCRIPTS/build zlib

cd $BUILD/$1*
./configure --host=$TARGET_NAME \
            --build=$HOST_NAME \
            --prefix=$LIB_PREFIX \
            --enable-static \
            --disable-shared \

make
make install
cp -P libpng-config $ROOT/$TOOLCHAIN/bin
