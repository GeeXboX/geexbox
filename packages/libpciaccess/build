#!/bin/sh

. config/options

$SCRIPTS/build toolchain

export have_mtrr_h=yes

cd $BUILD/$1*
./configure --host=$TARGET_NAME \
            --build=$HOST_NAME \
            --prefix=$LIB_PREFIX \
            --enable-shared \
            --disable-static \
            --with-pciids-path=/usr/share \
            --with-zlib \

make
strip_libs src
make install
