PKG_NAME=gcc-final
PKG_VERSION=4.4.4
PKG_REV=1
PKG_BUILD_DEPENDS="ccache make sed binutils gmp mpfr ppl cloog-ppl libelf linux-headers $TARGET_LIBC"
PKG_DEPENDS="$TARGET_LIBC"
PKG_PRIORITY=required
PKG_SECTION=libs
PKG_SHORTDESC="GCC support library"
PKG_LONGDESC="Shared version of the support library, a library of internal subroutines that GCC uses to overcome shortcomings of particular machines, or special needs for some languages."
