#!/bin/sh

. config/options

mkdir -p $INSTALL/usr/lib
cp -PR $BUILD/$1*/src/lib/hal/.libs/libehal.so*[.0-9] $INSTALL/usr/lib
cp -PR $BUILD/$1*/src/lib/notification/.libs/libenotify.so*[.0-9] $INSTALL/usr/lib
cp -PR $BUILD/$1*/src/lib/nm/.libs/libenm.so*[.0-9] $INSTALL/usr/lib
cp -PR $BUILD/$1*/src/lib/dbus/.libs/libedbus.so*[.0-9] $INSTALL/usr/lib
