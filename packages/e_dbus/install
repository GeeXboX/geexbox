#!/bin/sh

. config/options

$SCRIPTS/install ecore

mkdir -p $INSTALL/usr/lib
cp -PR $BUILD/$1*/src/lib/hal/.libs/libehal.so* $INSTALL/usr/lib
cp -PR $BUILD/$1*/src/lib/notification/.libs/libenotify.so* $INSTALL/usr/lib
cp -PR $BUILD/$1*/src/lib/nm/.libs/libenm.so* $INSTALL/usr/lib
cp -PR $BUILD/$1*/src/lib/dbus/.libs/libedbus.so* $INSTALL/usr/lib
