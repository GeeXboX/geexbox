#!/bin/sh

. config/options

$SCRIPTS/build toolchain
$SCRIPTS/build uboot-mkimage

cd $BUILD/$1*

make CROSS_COMPILE=${TARGET_PREFIX} touchbook_config
make CROSS_COMPILE=${TARGET_PREFIX}

mkimage -A arm -O linux -T script -C none -a 0 -e 0 -n 'GeeXboX' \
 -d $ROOT/$PACKAGES/$1/config/boot.cmd boot.scr
