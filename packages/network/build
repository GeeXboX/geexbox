#!/bin/sh

. config/options

$SCRIPTS/build bftpd
[ "$SAMBA" = yes -o "$SAMBA_SERVER" = yes ] && $SCRIPTS/build samba
$SCRIPTS/build wget

if [ "$WIFI" = yes ]; then
  $SCRIPTS/build wireless_tools
  $SCRIPTS/build acx
  [ "$NDISWRAPPER" = yes ] && $SCRIPTS/build ndiswrapper
  [ "$WPA_SUPPLICANT" = yes ] && $SCRIPTS/build wpa_supplicant
  [ "$OLSR" = yes ] && $SCRIPTS/build olsrd
fi

exit 0
