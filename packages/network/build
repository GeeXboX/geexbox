#!/bin/sh

. config/options

$SCRIPTS/build bftpd
$SCRIPTS/build samba

if [ "$WIFI" = yes ]; then
  $SCRIPTS/build wireless_tools
  $SCRIPTS/build acx
  $SCRIPTS/build madwifi-ng
  [ "$NDISWRAPPER" = yes ] && $SCRIPTS/build ndiswrapper
  [ "$WPA_SUPPLICANT" = yes ] && $SCRIPTS/build wpa_supplicant
fi

exit 0
