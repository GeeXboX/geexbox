#!/bin/sh

. config/options

$SCRIPTS/build bftpd
$SCRIPTS/build samba

if [ "$WIFI" = yes ]; then
  $SCRIPTS/build wireless_tools
# $SCRIPTS/build rt2400
# $SCRIPTS/build rt2500
# $SCRIPTS/build rt2570
  $SCRIPTS/build rt61
  $SCRIPTS/build rt73
  $SCRIPTS/build acx
  $SCRIPTS/build madwifi-ng
  $SCRIPTS/build iwlwifi
  [ "$NDISWRAPPER" = yes ] && $SCRIPTS/build ndiswrapper
  [ "$WPA_SUPPLICANT" = yes ] && $SCRIPTS/build wpa_supplicant
fi

exit 0
