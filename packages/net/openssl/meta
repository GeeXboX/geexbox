PKG_NAME=openssl
PKG_VERSION=1.0.2h
PKG_URL="http://www.openssl.org/source/${PKG_NAME}-${PKG_VERSION}.tar.gz"
PKG_REV="2"
PKG_DEPENDS="zlib cryptodev"
PKG_RUN_DEPENDS="$TARGET_LIBC openssl-certs"
PKG_BUILD_DEPENDS="toolchain makedepend"
PKG_PRIORITY=optional
PKG_SECTION=libs
PKG_SHORTDESC="Secure Socket Layer (SSL) library"
PKG_LONGDESC="This is the OpenSSL implementation of Secure Socket Layer (SSL)."
PKG_SECTION=net
