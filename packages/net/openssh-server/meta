PKG_PARENT=openssh-client
PKG_NAME=openssh-server
PKG_VERSION=6.6p1
PKG_URL="ftp://ftp.openbsd.org/pub/OpenBSD/OpenSSH/portable/openssh-${PKG_VERSION}.tar.gz"
PKG_REV=1
PKG_RUN_DEPENDS="$TARGET_LIBC ${PKG_PARENT}"
PKG_BUILD_DEPENDS="toolchain ${PKG_PARENT}"
PKG_DEPENDS="zlib openssl ${PKG_PARENT}"
PKG_PRIORITY=optional
PKG_SECTION=net
PKG_SHORTDESC="ssh server."
PKG_LONGDESC="ssh server (SSH server)"

