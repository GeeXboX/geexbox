PKG_NAME=ncurses-extra
PKG_PARENT=ncurses-base
PKG_NO_PARENT_DEPENDS=yes
PKG_RUN_DEPENDS="$PKG_PARENT"
PKG_PRIORITY=optional
PKG_SECTION=libs
PKG_SHORTDESC="Extended terminal database for ncurses and ncursesw"
PKG_LONGDESC="This package contains additional terminal definitions for ncurses and ncursesw."
