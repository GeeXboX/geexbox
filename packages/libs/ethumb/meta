PKG_NAME=ethumb
PKG_VERSION=`( get_meta EFL; echo $PKG_VERSION )`
PKG_URL="http://download.enlightenment.org/releases/${PKG_NAME}-${PKG_VERSION}.tar.bz2"
PKG_REV=1
PKG_DEPENDS="$TARGET_LIBC eina evas ecore edje edbus libexif"
PKG_BUILD_DEPENDS="toolchain"
PKG_PRIORITY=optional
PKG_SECTION=libs
PKG_SHORTDESC="Ethumb - Thumbnail generation library"
PKG_LONGDESC="Ethumb - Thumbnail generation library"
