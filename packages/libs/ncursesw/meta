PKG_NAME=ncursesw
PKG_PARENT=ncurses-base
PKG_NO_PARENT_DEPENDS=yes
PKG_RUN_DEPENDS="$TARGET_LIBC $PKG_PARENT"
PKG_PRIORITY=optional
PKG_SECTION=libs
PKG_SHORTDESC="Shared libraries for terminal handling (wide-char)"
PKG_LONGDESC="This package contains the shared libraries necessary to run programs compiled with ncursesw."
