#!/bin/sh

. config/options

$SCRIPTS/install libXau
$SCRIPTS/install libXdmcp
$SCRIPTS/install libX11
$SCRIPTS/install libxkbfile
$SCRIPTS/install libfontenc
$SCRIPTS/install libXfont
$SCRIPTS/install libXext
$SCRIPTS/install libXv
$SCRIPTS/install libXvMC
$SCRIPTS/install libdrm
$SCRIPTS/install libXrender
$SCRIPTS/install libXrandr
$SCRIPTS/install libpciaccess
[ "$MESA" = yes ] && $SCRIPTS/install mesa

exit 0
