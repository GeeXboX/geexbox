PKG_BUILD_DEPENDS="toolchain"
PKG_PRIORITY=required
PKG_SECTION=libs
PKG_SHORTDESC="XML parsing C library"
PKG_LONGDESC="This package contains the runtime, shared library of expat, the C library for parsing XML."
