#!/bin/sh

. config/options

sed -i -e "s%^#\(math.*\)%\1%" \
       -e "s%^#\(_random.*\)%\1%" \
       -e "s%^#\(binascii.*\)%\1%" \
       -e "s%^#\(unicodedata.*\)%\1%" \
       -e "s%^#\(time.*\)%\1%" \
       -e "s%^#\(cStringIO.*\)%\1%" \
       -e "s%^#\(cPickle.*\)%\1%" \
       -e "s%^#\(fcntl.*\)%\1%" \
       -e "s%^#\(array.*\)%\1%" \
       -e "s%^#\(cmath.*\)%\1%" \
       -e "s%^#\(struct.*\)%\1%" \
       -e "s%^#\(operator.*\)%\1%" \
       -e "s%^#\(_weakref.*\)%\1%" \
       -e "s%^#\(_testcapi.*\)%\1%" \
       -e "s%^#\(collections.*\)%\1%" \
       -e "s%^#\(itertools.*\)%\1%" \
       -e "s%^#\(strop.*\)%\1%" \
       -e "s%^#\(_socket.*\)%\1%" \
       $BUILD/$1*/Modules/Setup.dist
