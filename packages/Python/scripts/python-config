#!/bin/sh

case $1 in
  --prefix) echo "/usr" ;;
  --exec-prefix) echo "/usr" ;;
  --includes) echo "-I%LIB_PREFIX%/include/python2.6" ;;
  --libs) echo "-lpthread -ldl -lutil -lm -lpython2.6" ;;
  --cflags) echo "-I%LIB_PREFIX%/include/python2.6 -DNDEBUG %TARGET_CFLAGS%" ;;
  --ldflags) echo "-L%LIB_PREFIX%/lib -lpthread -ldl -lutil -lm -lpython2.6" ;;
esac

exit 0
