#!/bin/sh

. config/options

export ac_cv_path_HAVELIBUSB=$ROOT/$TOOLCHAIN/bin/libusb-config

cd $BUILD/$1*
LIBS="-lusb" ./configure --host=$TARGET_NAME \
            --build=$HOST_NAME \
            --prefix=/usr \
            --enable-static \
            --disable-shared \
            --disable-libftdipp \

make
strip_libs src
make_install
